{
  "name": "cobalttest",
  "version": "1.0.0",
  "description": "Simple test app",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:dev": "npx cross-env NODE_ENV=development webpack --progress --config \"./config/webpack.config.dev.client.js\"",
    "build:prod": " npx cross-env NODE_ENV=production webpack --progress --config webpack.prod.js",
    "dev-server": "webpack --watch --config \"./config/webpack.config.dev.client.js\"",
    "node-server": "nodemon --exec babel-node --presets=env -- ./src/server/index.js",
    "dev": "concurrently --kill-others \"npm run dev-server\" \"npm run node-server\"",
    "prod": "npx cross-env NODE_ENV=production webpack-dev-server --env=prod --config webpack.config.js",
    "clean": "rimraf dist"
  },
  "engines": {
    "node": "8.11.2",
    "npm": "6.1.0"
  },
  "author": "n0f3",
  "license": "ISC",
